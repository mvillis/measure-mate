{
  "name": "measure-mate",
  "description": "Simple tool to track maturity assessments.",
  "repository": "https://github.com/mvillis/measure-mate",
  "website": "https://github.com/mvillis/measure-mate",
  "license": "MIT",
  "env": {
    "DJANGO_DEBUG": {
      "required": false
    },
    "DJANGO_SETTINGS_MODULE": {
      "required": true
    },
    "DJANGO_SECRET_KEY": {
      "required": true,
      "generator": "secret"
    }
  },
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "heroku/python"
    }
  ]
}
